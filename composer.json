{
  "name": "masterfermin02/php-starter-kit",
  "description": "This project will help you to start a new project in PHP",
  "keywords": ["Blog", "PHP", "No frameworks", "starter", "startup", "starter-kit", "start-kit", "startup-kit"],
  "license": "MIT",
  "authors": [
    {
      "name": "Fermin Perdomo",
      "email": "masterfermin02@gmail.com",
      "role": "Creator / Main Developer"
    }
  ],
  "require": {
    "php": "~8.2",
    "filp/whoops": "~2.15",
    "nikic/fast-route": "^1.3",
    "rdlowrey/auryn": "^1.4",
    "mustache/mustache": "^2.14",
    "twig/twig": "~2.15",
    "erusev/parsedown": "^1.7",
    "twig/markdown-extra": "^3.5",
    "fperdomo/http": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.4"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
